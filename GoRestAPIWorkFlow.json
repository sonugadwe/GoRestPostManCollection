			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const resonse = pm.response.json();\r",
							"const user_id = resonse.id;\r",
							"console.log(\"User Id ==>\" + user_id);\r",
							"pm.variables.set(\"userID\", user_id);\r",
							"//local variable - userID\r",
							"\r",
							"//test case\r",
							"pm.test(\"Status code is 201\", function () {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"pm.test(\"check your name\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.name).to.eql(\"seema\");\r",
							"});\r",
							"pm.test(\"Content-Type is present\", function () {\r",
							"    pm.response.to.have.header(\"Content-Type\");\r",
							"});"
